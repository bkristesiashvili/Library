@model SectorListViewModel
<div>
    <div>
        <form onsubmit="return false;" method="post" data-delete-sector="@Model.Id" asp-controller="@SectorIndexLink" asp-action="delete" enctype="application/x-www-form-urlencoded">
            <input asp-for="Id" />
            <button type="button" onclick="sectorDelete('@Model.Id')" class="btn btn-sm text-11px text-danger">
                <i class="fas fa-trash"></i>
            </button>
        </form>
    </div>
</div>